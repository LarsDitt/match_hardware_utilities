<launch>
    <include file="$(find ur_hardware_helper)/launch/ur.launch">
        <arg name="controllers_started" value="franka_state_controller cartesian_controller"/> 
        <arg name="pose_filename" value="poses/panda_teached_poses.yaml"/>
    </include>
   
    <node name="ee_pose_marker" pkg="cartesian_move" type="ee_pose_marker.py">
        <remap from="pose" to="cartesian_controller/equilibrium_pose"/>
        <param name="base_frame" value="panda_link0"/>
        <param name="ee_frame" value="panda_EE"/>
    </node>
</launch>
<?xml version="1.0" ?>
<launch>
  <arg name="robot_name" default=""/>
  <arg name="robot_x"   default="0.0" />
  <arg name="robot_y"   default="0.0" />
  <arg name="robot_yaw" default="0.0" />

  <include file="$(find mir_gazebo)/launch/mir.launch" ns="$(arg robot_name)" pass_all_args="true">
    <arg name="tf_prefix" value="$(arg robot_name)"/>
    <arg name="model_name" value="$(arg robot_name)"/>
  </include>
 
</launch>
<launch>
    <arg name="tf_prefix" default=""/>

    <node   name="reconfigure_load_stiffness" pkg="dynamic_reconfigure" type="dynparam" 
                args="load fixed_joint_impedance_controller $(find panda_hardware_helper)/config/fixed_joint_impedance_dyn.yaml" />

    <include file="$(find panda_hardware_helper)/launch/panda.launch" pass_all_args="true">
        <arg name="controllers_started" default="franka_state_controller fixed_joint_impedance_controller"/>       
    </include>

</launch>
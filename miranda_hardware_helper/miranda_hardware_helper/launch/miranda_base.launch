<launch>

    <include file="$(find mir_hardware_helper)/launch/mir.launch" ns="miranda/mir">
        <arg name="tf_prefix" value="miranda/mir"/>    
        <arg name="move_base" value="true"/>   
    </include>

    <!-- Add a Transformation from Mir to panda for closed tf tree -->
    <node pkg="tf2_ros" type="static_transform_publisher" name="mir_ur_transformer" 
         args="0.35 -0.15 0.35 0 0 1 0 $(arg tf_prefix)/mir/base_link $(arg tf_prefix)/panda/link0" ns="miranda" />

</launch>
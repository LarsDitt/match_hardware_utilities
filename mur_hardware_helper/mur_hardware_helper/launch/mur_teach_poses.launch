<launch>
    <include file="$(find mur_hardware_helper)/launch/mur.launch">
        <arg name="controllers_started" value="franka_state_controller cartesian_controller"/> 
        <arg name="pose_filename" value="poses/mur_teached_poses.yaml"/>
    </include>
   
    <node name="ee_pose_marker" pkg="cartesian_move" type="ee_pose_marker.py">
        <remap from="pose" to="cartesian_controller/equilibrium_pose"/>
        <param name="base_frame" value="mur/ur/base_link"/>
        <param name="ee_frame" value="mur/ur/tool0"/>
    </node>
</launch>